{
  "name": "Mean",
  "description": "Calculates the mean value of data points in a cell.",
  "tags": [
    "descriptive statistics"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/mean_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Sample",
            "type": "",
            "description": "Sample identifiers",
            "ontologyMapping": "observation",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers",
            "ontologyMapping": "variable",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Y-Axis Measurement",
                "type": "numeric",
                "description": "Measurement value, per cell",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              },
              {
                "kind": "MetaFactor",
                "name": "X-Axis Measurement (Optional)",
                "type": "numeric",
                "description": "Second measurement value, per cell (optional)",
                "ontologyMapping": "measurement",
                "crosstabMapping": "x",
                "cardinality": "0..1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "ConditionalJoinSpec",
        "description": "Output varies based on X-axis presence",
        "alternatives": [
          {
            "kind": "OutputAlternative",
            "condition": "No X-axis (hasNumericXAxis = false)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Sample",
                      "Variable"
                    ],
                    "rColumns": [
                      "Sample",
                      "Variable"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "value",
                        "type": "double"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "kind": "OutputAlternative",
            "condition": "With X-axis (hasNumericXAxis = true)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [
                      "Sample",
                      "Variable"
                    ],
                    "rColumns": [
                      "Sample",
                      "Variable"
                    ]
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "mean_y",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "mean_x",
                        "type": "double"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ],
        "refactorSuggestion": "Standardize to always output 'mean' column"
      }
    ]
  }
}